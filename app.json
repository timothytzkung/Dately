{
  "expo": {
    "name": "Dately",
    "slug": "dately",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dately",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "NSCalendarsUsageDescription": "Allow Dately to access your calendar to add date reminders.",
        "NSLocationWhenInUseUsageDescription": "Dately needs your location to recommend nearby date venues.",
        "NSLocationAlwaysUsageDescription": "Dately needs your location to recommend nearby date venues.",
        "NSPhotoLibraryUsageDescription": "Dately needs access to your photo library to save your captured date memories."
      },
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyA5wU0rvR_p-KVRnA01xBHl_zfk_d9TZ8o"
      },
      "bundleIdentifier": "com.tka73.dately"
    },
    "android": {
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA5wU0rvR_p-KVRnA01xBHl_zfk_d9TZ8o"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icon.png",
        "backgroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tka73.dately"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Dately to access your camera to take photos of your dates."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Dately to use your location to find nearby date venues."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Dately to access your photos to add memories to your scrapbook.",
          "cameraPermission": "Allow Dately to take photos."
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow Dately to access your calendar to add date reminders."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
